{
  "name": "bombsquad-filespec",
  "version": "0.2.0",
  "description": "parsers for bob/cob files",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run build:bob && npm run build:cob",
    "build:bob": "npm run build:bob:py && npm run build:bob:js",
    "build:cob": "npm run build:cob:py && npm run build:cob:js",
    "build:py": "npm run build:bob:py && npm run build:cob:py",
    "build:js": "npm run build:bob:js && npm run build:cob:js",
    "build:bob:py": "kaitai-struct-compiler -t python --outdir=output/python/ src/bob.ksy",
    "build:bob:js": "kaitai-struct-compiler -t javascript --outdir=output/javascript/ src/bob.ksy",
    "build:cob:py": "kaitai-struct-compiler -t python --outdir=output/python/ src/cob.ksy",
    "build:cob:js": "kaitai-struct-compiler -t javascript --outdir=output/javascript/ src/cob.ksy"
  },
  "author": "aryan02420",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aryan02420/bombsquad-filespec.git"
  },
  "keywords": [
    "bombsquad"
  ],
  "bugs": {
    "url": "https://github.com/aryan02420/bombsquad-filespec/issues"
  },
  "homepage": "https://github.com/aryan02420/bombsquad-filespec#readme",
  "dependencies": {
    "kaitai-struct": "^0.9.0"
  }
}
